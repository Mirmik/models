$fn = 40;

linear_extrude(height = 2) {
	union() {
		union() {
			translate(v = [2, 2, 0]) {
				offset(r = 2) {
					square(size = [4, 16]);
				}
			}
			translate(v = [4.0000000000, 10.0000000000, 0]) {
				square(size = [4.0000000000, 10.0000000000]);
			}
		}
		translate(v = [0.0000000000, 10.0000000000, 0]) {
			square(size = [4.0000000000, 10.0000000000]);
		}
	}
}